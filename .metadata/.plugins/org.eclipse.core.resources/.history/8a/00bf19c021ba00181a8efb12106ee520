package edu.mitrofanov.codejam.r1a_2008.task_b;

import java.io.File;
import java.util.Scanner;

import edu.mitrofanov.codejam.utils.FileUtils;

public class Main {
	//public final static String INPUT_FILE = "resources/B-test.in";
	//public final static String OUTPUT_FILE = "resources/B-test.out";
	public final static String INPUT_FILE = "resources/B-small-practice.in";
	public final static String OUTPUT_FILE = "resources/B-small-practice.out";

	@SuppressWarnings("resource")
	public static void main(String[] args) throws Exception {
		Scanner sc = new Scanner(new File(INPUT_FILE));
		int C = Integer.parseInt(sc.nextLine());
		System.out.println("start");
		String[] output = new String[C];
		for (int i =0; i< C; i++) {
			int N = Integer.parseInt(sc.nextLine());
			int custNum = Integer.parseInt(sc.nextLine());
			String[][] custShakesData = new String[custNum][];
			for (int j = 0; j < custShakesData.length; j++) {
				int custShakes = sc.nextInt();
				custShakesData[j] = new String[custShakes];
				for (int k = 0; k<custShakes; k++){
					String shakeData = "" + sc.nextInt() + " " + sc.nextInt();
					custShakesData[j][k] = shakeData;
				}
				sc.nextLine();
			}
			output[i] = "Case #" + (i+1) + ": " + TaskResolver.resolveOneCase(N, custShakesData);
			System.out.println(output[i]);
			System.out.println("---------------------------------------------------------------");
		}
		FileUtils.writeStringArrayToFile(output, OUTPUT_FILE);
	}
	
}
